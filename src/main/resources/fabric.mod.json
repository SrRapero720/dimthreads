{
  "schemaVersion": 1,
  "id": "$modid",
  "version": "$modversion",
  "name": "$modname",
  "description": "$mod_description",
  "authors": [
    "$modauthor"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/dimthreads",
    "sources": "https://github.com/${github_project}",
    "issues": "https://github.com/${github_project}/issues"
  },
  "license": "$modlicense",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "me.srrapero720.dimthread.DimThread"
    ]
  },
  "mixins": [
    "$modid.$mixin_file_suffix"
  ],
  "accessWidener": "dimthread.accesswidener",
  "depends": {
    "fabricloader": ">=0.16",
    "minecraft": "~1.21.1",
    "java": ">=21",
    "fabric-api": "*"
  }
}
